{
  "name": "bcx-aurelia-dnd",
  "version": "1.4.0",
  "description": "Buttonwood Aurelia Drag and Drop",
  "keywords": [
    "aurelia",
    "aurelia-dnd",
    "dnd",
    "drag-and-drop"
  ],
  "homepage": "http://github.com/buttonwoodcx/bcx-aurelia-dnd",
  "bugs": {
    "url": "https://github.com/buttonwoodcx/bcx-aurelia-dnd/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/buttonwoodcx/bcx-aurelia-dnd"
  },
  "license": "MIT",
  "author": "Chunpeng Huo",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@zeit/ncc": "^0.22.2",
    "babel-eslint": "^10.1.0",
    "babelify": "^10.0.0",
    "browser-do": "^2.0.2",
    "browserify": "^16.5.1",
    "eslint": "^7.0.0",
    "jquery": "^3.5.1",
    "standard-changelog": "^2.0.24",
    "tap-dot": "^2.0.0",
    "tape": "^5.0.0"
  },
  "scripts": {
    "prebuild": "ncc build src/index.js -m -e aurelia-event-aggregator -o packed",
    "build": "babel packed/index.js -o dist/index.js",
    "lint": "eslint src",
    "prepare": "npm run build",
    "preversion": "npm test",
    "version": "standard-changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "pretest": "npm run lint",
    "test": "browserify -t babelify test/dnd.spec.js test/preview-drawers.spec.js | browser-do --tap"
  },
  "main": "dist/index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "dependencies": {
    "aurelia-event-aggregator": "^1.0.3"
  }
}
